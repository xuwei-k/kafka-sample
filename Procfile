kafka1: rm -rf /tmp/kafka-logs-1 && kafka-install/$KAFKA_VERSION/bin/kafka-server-start.sh server1.properties
kafka2: rm -rf /tmp/kafka-logs-2 && kafka-install/$KAFKA_VERSION/bin/kafka-server-start.sh server2.properties
kafka3: rm -rf /tmp/kafka-logs-3 && kafka-install/$KAFKA_VERSION/bin/kafka-server-start.sh server3.properties
zookeeper: rm -rf /tmp/zookeeper && kafka-install/$KAFKA_VERSION/bin/zookeeper-server-start.sh kafka-install/$KAFKA_VERSION/config/zookeeper.properties
